hsqldb

CREATE TABLE IF NOT EXISTS usuario(id INT IDENTITY NOT NULL, nome VARCHAR(100) NOT NULL, email VARCHAR(50) NOT NULL, senha VARCHAR(20) NOT NULL, PRIMARY KEY(id));
CREATE TABLE IF NOT EXISTS telefone(id INT IDENTITY NOT NULL , idUsuario INT NOT NULL, ddd INT NOT NULL, numero VARCHAR(13) NOT NULL, tipo VARCHAR(20) NOT NULL,PRIMARY KEY(id));

postgres

CREATE TABLE IF NOT EXISTS usuario(id BIGSERIAL, nome VARCHAR(100) NOT NULL, email VARCHAR(50) NOT NULL, senha VARCHAR(20) NOT NULL, PRIMARY KEY(id));
CREATE TABLE IF NOT EXISTS telefone(id BIGSERIAL, idUsuario INT NOT NULL, ddd INT NOT NULL, numero VARCHAR(13) NOT NULL, tipo VARCHAR(20) NOT NULL,PRIMARY KEY(id));
